- set_fact: { GREP_FILE: "{{ SEMAPHORE_PATH }}/.env" }
- debug: { var: [ GREP_FILE, grep_regex ] }

- name: _register_grep_in_grep_file | Run grep check with grep regex {{ grep_regex }}
  ansible.builtin.shell: grep -oE "{{ grep_regex }}" "{{ GREP_FILE }}"
  register: grep_result
  ignore_errors: true
  changed_when: false